jQuery(document).ready(function() {	
    animateShadow();
});

function animateShadow(){
    
    $('.portfolio-gallery a').mouseenter(function() {
        $(this).stop(true,true).animate({boxShadow: '0 0 15px rgba(0, 0, 0, 0.1)'}, 200);
        $(this).find('img').animate({opacity: 0.7}, 200);
    
    }).mouseleave(function(){
        $(this).stop(true,true).animate({boxShadow: '0 0 0'}, 200);
        $(this).find('img').animate({opacity: 1}, 200);
    });
    
    $('.post .img.animate').mouseenter(function() {
        $(this).stop(true,true).animate({boxShadow: '0 0 15px rgba(0, 0, 0, 0.1)'}, 200);
        $(this).find('img').animate({opacity: 0.7}, 200);
    
    }).mouseleave(function(){
        $(this).stop(true,true).animate({boxShadow: '0 0 0'}, 200);
        $(this).find('img').animate({opacity: 1}, 200);
    });
    
    $('.about .item').mouseenter(function() {
        $(this).stop(true,true).animate({backgroundColor:'#fcfcfc', boxShadow: '0 0 15px rgba(0, 0, 0, 0.1)'}, 200);
        $(this).find('.social').stop(true,true).animate({backgroundColor:'#fafafa'}, 200);
    
    }).mouseleave(function(){
        $(this).stop(true,true).animate({backgroundColor:'#fafafa', boxShadow: '0 0 0'}, 200);
        $(this).find('.social').stop(true,true).animate({backgroundColor:'#f5f5f5'}, 200)
    });
    
    $('#container .element').mouseenter(function() {
        $(this).stop(true,true).animate({backgroundColor:'#fcfcfc', boxShadow: '0 0 15px rgba(0, 0, 0, 0.1)'}, 200);
        $(this).find('img').stop(true,true).animate({opacity: 0.7}, 200);
        $(this).find('h4').addClass('hover');
    
    }).mouseleave(function(){
        $(this).stop(true,true).animate({backgroundColor:'#fafafa', boxShadow: '0 0 0'}, 200);
        $(this).find('img').stop(true,true).animate({opacity: 1}, 200);
        $(this).find('h4').removeClass('hover');
    });
    
    $('.woocommerce li.product').mouseenter(function() {
        $(this).stop(true,true).animate({backgroundColor:'#fcfcfc', boxShadow: '0 0 15px rgba(0, 0, 0, 0.1)'}, 200);
    }).mouseleave(function(){
        $(this).stop(true,true).animate({backgroundColor:'#fafafa', boxShadow: '0 0 0'}, 200);
    });

}
