jQuery(document).ready(function() {	
    footerClear();
    footerFooter_();
});

function footerClear() {
    $('<div class="clear"></div>').insertAfter("#footer .grid_3:nth-child(4n)");
    $('<div class="clear"></div>').insertAfter("#footer .grid_4:nth-child(3n)");
}


function footerFooter_() {
    var $minHeight = 648;
    var $footerHeight = $('#footer').height(); 
    if ($footerHeight > $minHeight){
        $('.footer_').addClass('bottom_gradient')  
    }
}
