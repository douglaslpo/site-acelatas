jQuery(document).ready(function() {	
    portfolioFilter();
});

function portfolioFilter() {
    
    var filters = $('#options');
    var filtersHeight = $('#options').height();

    if(filtersHeight>18){
        filters.addClass('bg2');
    }                         
    
}
function portfolioFilter() {
    $('.showbiz .item').mouseenter(function() {
        $(this).find('h6').addClass('hover');
    
    }).mouseleave(function(){
        $(this).find('h6').removeClass('hover');
    });
}
